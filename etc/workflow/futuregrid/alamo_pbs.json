{
  "name": "alamo_pbs",
  "steps": [
    { "name": "ipf.resource_name.ResourceNameStep",
      "params": {
        "resource_name": "torquemoab.alamo.futuregrid.org"
      }
    },
    { "name": "ipf.site_name.SiteNameStep",
      "params": {
        "site_name": "tacc.futuregrid.org"
      }
    },
    { "name": "glue2.gram_endpoint.Gram5EndpointStep",
      "params": {
        "endpoint": {
          "Name": "gram5-fork",
          "URL": "gram://daemon.alamo.futuregrid.org:2119/jobmanager-fork"
        }
      }
    },
    { "name": "glue2.gram_endpoint.Gram5EndpointStep",
      "params": {
        "endpoint": {
          "Name": "gram5-pbs",
          "URL": "gram://daemon.alamo.futuregrid.org:2119/jobmanager-pbs"
        }
      }
    },
    { "name": "glue2.genesis_endpoint.GenesisIIEndpointStep",
      "params": {
        "endpoint": {
          "URL": "https://daemon.alamo.futuregrid.org:18443/axis/services/GeniiBESPortType?genii-container-id=98B3AC57-D09F-63F1-6EE6-2664E1EF9699"
        }
      }
    },
    { "name": "glue2.pbs.pbs_computing_activity.PbsComputingActivitiesStep" },
    { "name": "glue2.pbs.pbs_computing_share.PbsComputingSharesStep" },
    { "name": "glue2.pbs.pbs_computing_service.PbsComputingServiceStep" },
    { "name": "glue2.pbs.pbs_execution_environment.PbsExecutionEnvironmentsStep" },
    { "name": "glue2.pbs.pbs_computing_manager.PbsComputingManagerStep" },
    {
      "name": "ipf.amqp.AmqpPublishStep",
      "params": {
        "publish": ["glue2.public_compute.PublicComputeIpfJson"],
        "services": [ "inca.futuregrid.org" ],
        "ssl_options": {
          "keyfile":"key.pem",
          "certfile":"cert.pem",
          "ca_certs":"ca_certs.pem"
        },
	"vhost": "monitoring",
        "exchange": "glue2.systems"
      }
    },
    {
      "name": "ipf.amqp.AmqpPublishStep",
      "params": {
        "publish": ["glue2.private_compute.PrivateComputeIpfJson"],
        "services": [ "inca.futuregrid.org" ],
        "ssl_options": {
          "keyfile":"key.pem",
          "certfile":"cert.pem",
          "ca_certs":"ca_certs.pem"
        },
	"vhost": "monitoring",
        "exchange": "glue2.activities"
      }
    },
    {
      "name": "ipf.file.FilePublishStep",
      "params": {
        "publish": ["glue2.public_compute.PublicComputeIpfJson"],
        "path": "glue2_pbs_public.json"
      }
    },
    {
      "name": "ipf.file.FilePublishStep",
      "params": {
        "publish": ["glue2.private_compute.PrivateComputeIpfJson"],
        "path": "glue2_pbs_private.json"
      }
    }
  ]
}
