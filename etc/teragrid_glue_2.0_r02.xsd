<?xml version="1.0" encoding="UTF-8"?>

<!--

The way this schema handles associations is as follows:

  * An association is only stored by inclusion as a sub-element when the included element is not associated
    elsewhere and the information in the two classes is expected to be updated simultaneously
  * Otherwise, an association is stored using a reference (ID_t) to another element, rather than using a sub-element
  * Right now, associations are stored in the complexType for both sides. I'll change this to typically storing
    an association on only one side. Here are some heuristics for this:
    * The side that has fewer associations to store per element should store the association
    * The side that is updated more frequently should store the association

Every concrete complexType that is not used as an element in another complexType has an element defined at the top
level of the schema. Therefore, documents containing a partial description of a system are valid.

-->

<schema targetNamespace="http://info.teragrid.org/glue/2009/02/spec_2.0_r02"
  xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:glue="http://info.teragrid.org/glue/2009/02/spec_2.0_r02"
  elementFormDefault="qualified">

  <element name="Entities" type="glue:Entities_t"/>

  <complexType name="Entities_t">
    <sequence>
      <element name="Location" type="glue:Location_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="Contact" type="glue:Contact_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="AdminDomain" type="glue:AdminDomain_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="UserDomain" type="glue:UserDomain_t" minOccurs="0" maxOccurs="unbounded"/>
      <!-- Typically use Compute* or Storage* instead of Service, Endpoint, and Activity. -->
      <element name="Service" type="glue:Service_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="Endpoint" type="glue:Endpoint_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="Activity" type="glue:Activity_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="AccessPolicy" type="glue:AccessPolicy_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="MappingPolicy" type="glue:MappingPolicy_t" minOccurs="0" maxOccurs="unbounded"/>

      <element name="ComputingService" type="glue:ComputingService_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="ComputingEndpoint" type="glue:ComputingEndpoint_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="ComputingShare" type="glue:ComputingShare_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="ComputingManager" type="glue:ComputingManager_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="Benchmark" type="glue:Benchmark_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="ExecutionEnvironment" type="glue:ExecutionEnvironment_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="ApplicationEnvironment" type="glue:ApplicationEnvironment_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="ApplicationHandle" type="glue:ApplicationHandle_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="ComputingActivity" type="glue:ComputingActivity_t" minOccurs="0" maxOccurs="unbounded"/>

      <element name="StorageService" type="glue:StorageService_t" minOccurs="0" maxOccurs="unbounded"/>
      <!-- include below in StorageService? It might be updated/measured differently... -->
      <element name="StorageServiceCapacity" type="glue:StorageServiceCapacity_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="StorageEndpoint" type="glue:StorageEndpoint_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="StorageShare" type="glue:StorageShare_t" minOccurs="0" maxOccurs="unbounded"/>
      <!-- include below in StorageShare? It might be updated/measured differently... -->
      <element name="StorageShareCapacity" type="glue:StorageShareCapacity_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="StorageManager" type="glue:StorageManager_t" minOccurs="0" maxOccurs="unbounded"/>
      <element name="DataStore" type="glue:DataStore_t" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <!-- BEGIN MAIN ENTITIES -->

  <complexType name="Extension_t">
    <simpleContent>
      <extension base="string">
        <attribute name="Key" type="string"/>
      </extension>
    </simpleContent>
  </complexType>

  <complexType name="Entity">
    <sequence>
      <element name="ID" type="glue:ID_t"/>
      <element name="Name" type="string" minOccurs="0"/>
      <element name="OtherInfo" type="string" minOccurs="0" maxOccurs="unbounded"/>
      <element name="Extension" type="glue:Extension_t" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
    <attribute name="CreationTime" type="dateTime"/>
    <attribute name="Validity" type="unsignedLong"/>
  </complexType>

  <complexType name="Location_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
          <element name="Address" type="string" minOccurs="0"/>
          <element name="Place" type="string" minOccurs="0"/>
          <element name="Country" type="string" minOccurs="0"/>
          <element name="PostCode" type="string" minOccurs="0"/>
          <element name="Latitude" type="glue:Latitude_t" minOccurs="0"/>
          <element name="Longitude" type="glue:Longitude_t" minOccurs="0"/>
	  <!-- associations -->
	  <element name="Service" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Domain" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="Contact_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
          <element name="URL" type="anyURI"/>
          <element name="Type" type="glue:ContactType_t"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType abstract="true" name="Domain_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
          <element name="Description" type="string" minOccurs="0"/>
          <element name="WWW" type="anyURI" minOccurs="0" maxOccurs="unbounded"/>
	  <!-- associations -->
	  <element name="Contact" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Location" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
 	<!-- <attribute name="BaseType" fixed="Domain" use="required"/> -->
      </extension>
    </complexContent>
  </complexType>

  <complexType name="AdminDomain_t">
    <complexContent>
      <extension base="glue:Domain_t">
        <sequence>
          <element name="Distributed" type="glue:ExtendedBoolean_t" minOccurs="0"/>
          <element name="Owner" type="string" minOccurs="0" maxOccurs="unbounded"/>
	  <!-- associations -->
	  <element name="Service" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="ChildAdminDomain" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="ParentAdminDomain" type="glue:ID_t" minOccurs="0"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="UserDomain_t">
    <complexContent>
      <extension base="glue:Domain_t">
        <sequence>
          <element name="Level" type="unsignedInt" minOccurs="0"/>
          <element name="UserManager" type="anyURI" minOccurs="0" maxOccurs="unbounded"/>
          <element name="Member" type="string" minOccurs="0" maxOccurs="unbounded"/>
	  <!-- associations -->
	  <element name="Policy" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="ChildUserDomain" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="ParentUserDomain" type="glue:ID_t" minOccurs="0"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="Service_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
          <element name="Capability" type="glue:Capability_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="Type" type="glue:ServiceType_t"/>
          <element name="QualityLevel" type="glue:QualityLevel_t"/>
          <element name="StatusInfo" type="anyURI" minOccurs="0" maxOccurs="unbounded"/>
          <element name="Complexity" type="string" minOccurs="0"/>
	  <!-- associations -->
	  <element name="Endpoint" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Share" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Manager" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Contact" type="glue:ID_t" minOccurs="0"/>
	  <element name="Location" type="glue:ID_t" minOccurs="0"/>
	  <element name="Service" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="Endpoint_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
          <element name="URL" type="anyURI"/>
          <element name="Capability" type="glue:Capability_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="Technology" type="glue:EndpointTechnology_t"/>
          <element name="InterfaceName" type="glue:InterfaceName_t"/>
          <element name="InterfaceVersion" type="anyURI" minOccurs="0" maxOccurs="unbounded"/>
          <element name="InterfaceExtension" type="anyURI" minOccurs="0" maxOccurs="unbounded"/>
          <element name="WSDL" type="anyURI" minOccurs="0" maxOccurs="unbounded"/>
          <element name="SupportedProfile" type="anyURI" minOccurs="0" maxOccurs="unbounded"/>
          <element name="Semantics" type="anyURI" minOccurs="0" maxOccurs="unbounded"/>
          <element name="Implementor" type="string" minOccurs="0"/>
          <element name="ImplementationName" type="string" minOccurs="0"/>
          <element name="ImplementationVersion" type="string" minOccurs="0"/>
          <element name="QualityLevel" type="glue:QualityLevel_t"/>
          <element name="HealthState" type="glue:EndpointHealthState_t"/>
          <element name="HealthStateInfo" type="string" minOccurs="0"/>
          <element name="ServingState" type="glue:ServingState_t"/>
          <element name="StartTime" type="dateTime" minOccurs="0"/>
          <element name="IssuerCA" type="glue:DN_t" minOccurs="0"/>
          <element name="TrustedCA" type="glue:DN_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="DowntimeAnnounce" type="dateTime" minOccurs="0"/>
          <element name="DowntimeStart" type="dateTime" minOccurs="0"/>
          <element name="DowntimeEnd" type="dateTime" minOccurs="0"/>
          <element name="DowntimeInfo" type="string" minOccurs="0"/>
	  <!-- associations -->
	  <element name="Service" type="glue:ID_t" minOccurs="0"/> <!-- set minOccurs because of specialization -->
	  <element name="Share" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="AccessPolicy" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Activity" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType abstract="true" name="Share_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
          <element name="Description" type="string" minOccurs="0"/>
	  <!-- associations -->
	  <element name="Endpoint" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Resource" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Service" type="glue:ID_t" minOccurs="0"/> <!-- set minOccurs because of specialization -->
	  <element name="Activity" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="MappingPolicy" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
        <!-- <attribute name="BaseType" fixed="Share" use="required"/> -->
      </extension>
    </complexContent>
  </complexType>

  <complexType abstract="true" name="Manager_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
          <element name="ProductName" type="string"/>
          <element name="ProductVersion" type="string" minOccurs="0"/>
	  <!-- associations -->
	  <element name="Service" type="glue:ID_t" minOccurs="0"/> <!-- set minOccurs because of specialization -->
	  <element name="Resource" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/> <!-- same -->
        </sequence>
        <!-- <attribute name="BaseType" fixed="Manager" use="required"/> -->
      </extension>
    </complexContent>
  </complexType>

  <complexType abstract="true" name="Resource_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
	  <!-- associations -->
	  <element name="Manager" type="glue:ID_t" minOccurs="0"/> <!-- set minOccurs because of specialization -->
	  <element name="Share" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <element name="Activity" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
        <!-- <attribute name="BaseType" fixed="Resource" use="required"/> -->
      </extension>
    </complexContent>
  </complexType>

  <complexType name="Activity_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
	  <!-- associations -->
	  <element name="UserDomain" type="glue:ID_t" minOccurs="0"/>
	  <element name="Endpoint" type="glue:ID_t" minOccurs="0"/>
	  <element name="Share" type="glue:LocalID_t" minOccurs="0"/>
	  <element name="Resource" type="glue:ID_t" minOccurs="0"/>
	  <element name="Activity" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType abstract="true" name="Policy_t">
    <complexContent>
      <extension base="glue:Entity">
        <sequence>
          <element name="Scheme" type="glue:PolicyScheme_t"/>
          <element name="Rule" type="string" maxOccurs="unbounded"/>
	  <!-- associations -->
	  <element name="UserDomain" type="glue:ID_t" minOccurs="0"/>
        </sequence>
        <!-- <attribute name="BaseType" fixed="Policy" use="required"/> -->
      </extension>
    </complexContent>
  </complexType>

  <complexType name="AccessPolicy_t">
    <complexContent>
      <extension base="glue:Policy_t">
        <sequence>
	  <!-- associations -->
	  <element name="Endpoint" type="glue:ID_t" minOccurs="0"/>
	</sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="MappingPolicy_t">
    <complexContent>
      <extension base="glue:Policy_t">
        <sequence>
	  <!-- associations -->
	  <element name="Share" type="glue:LocalID_t" minOccurs="0"/>
	</sequence>
      </extension>
    </complexContent>
  </complexType>
  <!--  END MAIN ENTITIES  -->

  <!--  BEGIN COMPUTING ENTITIES  -->
  <complexType name="ComputingService_t">
    <complexContent>
      <extension base="glue:Service_t">
        <sequence>
          <element name="TotalJobs" type="unsignedInt" minOccurs="0"/>
          <element name="RunningJobs" type="unsignedInt" minOccurs="0"/>
          <element name="WaitingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="StagingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="SuspendedJobs" type="unsignedInt" minOccurs="0"/>
          <element name="PreLRMSWaitingJobs" type="unsignedInt" minOccurs="0"/>
	  <!-- associations -->
          <element name="ComputingEndpoint" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="ComputingShare" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="ComputingManager" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="StorageService" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
	  <!-- this association isn't listed for this class in the spec -->
          <element name="ToStorageService" type="glue:ToStorageService_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="ComputingEndpoint_t">
    <complexContent>
      <extension base="glue:Endpoint_t">
        <sequence>
          <element name="Staging" type="glue:Staging_t" minOccurs="0"/>
          <element name="JobDescription" type="glue:JobDescription_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="TotalJobs" type="unsignedInt" minOccurs="0"/>
          <element name="RunningJobs" type="unsignedInt" minOccurs="0"/>
          <element name="WaitingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="StagingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="SuspendedJobs" type="unsignedInt" minOccurs="0"/>
          <element name="PreLRMSWaitingJobs" type="unsignedInt" minOccurs="0"/>
	  <!-- associations -->
          <element name="ComputingService" type="glue:ID_t"/>
          <element name="ComputingShare" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="ComputingActivity" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="ComputingShare_t">
    <complexContent>
      <extension base="glue:Share_t">
        <sequence>
          <element name="MappingQueue" type="string" minOccurs="0"/>
          <element name="MaxWallTime" type="unsignedLong" minOccurs="0"/>
          <element name="MaxMultiSlotWallTime" type="unsignedLong" minOccurs="0"/>
          <element name="MinWallTime" type="unsignedLong" minOccurs="0"/>
          <element name="DefaultWallTime" type="unsignedLong" minOccurs="0"/>
          <element name="MaxCPUTime" type="unsignedLong" minOccurs="0"/>
          <element name="MaxTotalCPUTime" type="unsignedLong" minOccurs="0"/>
          <element name="MinCPUTime" type="unsignedLong" minOccurs="0"/>
          <element name="DefaultCPUTime" type="unsignedLong" minOccurs="0"/>
          <element name="MaxTotalJobs" type="unsignedInt" minOccurs="0"/>
          <element name="MaxRunningJobs" type="unsignedInt" minOccurs="0"/>
          <element name="MaxWaitingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="MaxPreLRMSWaitingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="MaxUserRunningJobs" type="unsignedInt" minOccurs="0"/>
          <element name="MaxSlotsPerJob" type="unsignedInt" minOccurs="0"/>
          <element name="MaxStateInStreams" type="unsignedInt" minOccurs="0"/>
          <element name="MaxStageOutStreams" type="unsignedInt" minOccurs="0"/>
          <element name="SchedulingPolicy" type="glue:SchedulingPolicy_t" minOccurs="0"/>
          <element name="MaxMainMemory" type="unsignedLong" minOccurs="0"/>
          <element name="GuaranteedMainMemory" type="unsignedLong" minOccurs="0"/>
          <element name="MaxVirtualMemory" type="unsignedLong" minOccurs="0"/>
          <element name="GuaranteedVirtualMemory" type="unsignedLong" minOccurs="0"/>
          <element name="MaxDiskSpace" type="unsignedLong" minOccurs="0"/>
          <element name="DefaultStorageService" type="anyURI" minOccurs="0"/>
          <element name="Preemption" type="glue:ExtendedBoolean_t" minOccurs="0"/>
          <element name="ServingState" type="glue:ServingState_t"/>
          <element name="TotalJobs" type="unsignedInt" minOccurs="0"/>
          <element name="RunningJobs" type="unsignedInt" minOccurs="0"/>
          <element name="LocalRunningJobs" type="unsignedInt" minOccurs="0"/>
          <element name="WaitingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="LocalWaitingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="SuspendedJobs" type="unsignedInt" minOccurs="0"/>
          <element name="LocalSuspendedJobs" type="unsignedInt" minOccurs="0"/>
          <element name="StagingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="PreLRMSWaitingJobs" type="unsignedInt" minOccurs="0"/>
          <element name="EstimatedAverageWaitingTime" type="unsignedLong" minOccurs="0"/>
          <element name="EstimatedWorstWaitingTime" type="unsignedLong" minOccurs="0"/>
          <element name="FreeSlots" type="unsignedInt" minOccurs="0"/>
          <element name="FreeSlotsWithDuration" type="string" minOccurs="0"/>
          <element name="UsedSlots" type="unsignedInt" minOccurs="0"/>
          <element name="RequestedSlots" type="unsignedInt" minOccurs="0"/>
          <element name="ReservationPolicy" type="glue:ReservationPolicy_t" minOccurs="0"/>
          <element name="Tag" type="string" minOccurs="0" maxOccurs="unbounded"/>
	  <!-- associations -->
          <element name="ComputingEndpoint" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="ExecutionEnvironment" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="ComputingService" type="glue:ID_t"/>
          <element name="ComputingActivity" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="ComputingManager_t">
    <complexContent>
      <extension base="glue:Manager_t">
        <sequence>
          <element name="Version" type="string" minOccurs="0"/>
          <element name="Reservation" type="glue:ExtendedBoolean_t" minOccurs="0"/>
          <element name="BulkSubmission" type="glue:ExtendedBoolean_t" minOccurs="0"/>
          <element name="TotalPhysicalCPUs" type="unsignedInt" minOccurs="0"/>
          <element name="TotalLogicalCPUs" type="unsignedInt" minOccurs="0"/>
          <element name="TotalSlots" type="unsignedInt" minOccurs="0"/>
          <element name="SlotsUsedByLocalJobs" type="unsignedInt" minOccurs="0"/>
          <element name="SlotsUsedByGridJobs" type="unsignedInt" minOccurs="0"/>
          <element name="Homogeneous" type="glue:ExtendedBoolean_t" minOccurs="0"/>
          <element name="Homogeneity" type="glue:ExtendedBoolean_t" minOccurs="0"/> <!-- bug in glue2 code -->
          <element name="NetworkInfo" type="glue:NetworkInfo_t" minOccurs="0"/>
          <element name="LogicalCPUDistribution" type="string" minOccurs="0"/>
          <element name="WorkignAreaShared" type="glue:ExtendedBoolean_t" minOccurs="0"/>
          <element name="WorkignAreaGuaranteed" type="glue:ExtendedBoolean_t" minOccurs="0"/>
          <element name="WorkignAreaTotal" type="unsignedLong" minOccurs="0"/>
          <element name="WorkingAreaFree" type="unsignedLong" minOccurs="0"/>
          <element name="WorkingAreaLifeTime" type="unsignedLong" minOccurs="0"/>
          <element name="WorkingAreaMultiSlotTotal" type="unsignedLong" minOccurs="0"/>
          <element name="WorkingAreaMultiSlotFree" type="unsignedLong" minOccurs="0"/>
          <element name="WorkingAreaMultiSlotLifeTime" type="unsignedLong" minOccurs="0"/>
          <element name="CacheTotal" type="unsignedLong" minOccurs="0"/>
          <element name="CacheFree" type="unsignedLong" minOccurs="0"/>
          <element name="TmpDir" type="string" minOccurs="0"/>
          <element name="ScratchDir" type="string" minOccurs="0"/>
          <element name="ApplicationDir" type="string" minOccurs="0"/>
	  <!-- associations -->
          <element name="ComputingService" type="glue:ID_t"/>
          <element name="ExecutionEnvironment" type="glue:ID_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="ApplicationEnvironment" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
          <element name="Benchmark" type="glue:LocalID_t" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="Benchmark_t">
    <complexContent>
      <extension base="glue:Entity">
	<sequence>
	  <element name="Type" type="glue:BenchmarkType_t"/>
	  <element name="Value" type="int"/>
	  <!-- associations -->
          <element name="ExecutionEnvironment" type="glue:ID_t" minOccurs="0"/>
          <element name="ComputingManager" type="glue:ID_t" minOccurs="0"/>
	</sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="ExecutionEnvironment_t">
    <complexContent>
      <extension base="glue:Resource_t">
        <sequence>
          <element name="Platform" type="glue:Platform_t"/>
          <element name="VirtualMachine" type="glue:ExtendedBoolean_t" minOccurs="0"/>
          <element name="TotalInstances" type="unsignedInt" minOccurs="0"/>
          <element name="UsedInstances" type="unsignedInt" minOccurs="0"/>
          <element name="UnavailableInstances" type="unsignedInt" minOccurs="0"/>
          <element name="PhysicalCPUs" type="unsignedInt" minOccurs="0"/>
          <element name="LogicalCPUs" type="unsignedInt" minOccurs="0"/>
          <element name="CPUMultiplicity" type="glue:CPUMultiplicity_t" minOccurs="0"/>
          <element name="CPUVendor" type="string" minOccurs="0"/>
          <element name="CPUModel" type="string" minOccurs="0"/>
          <element name="CPUVersion" type="string" minOccurs="0"/>
          <element name="CPUClockSpeed" type="unsignedInt" minOccurs="0"/>
          <element name="CPUTimeScalingFactor" type="float" minOccurs="0"/>
          <element name="WallTimeScalingFactor" type="float" minOccurs="0"/>
          <element name="MainMemorySize" type="unsignedLong"/>
